    "axios": "^1.13.2",
    "axios-retry": "^4.5.0",
    "bcrypt": "^5.1.0",
    "bull": "^4.16.5",
    "cloudinary": "^2.0.0",
    "cors": "^2.8.5",
    "csrf-csrf": "^4.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.3.1",
    "firebase-admin": "^11.0.0",
    "handlebars": "^4.7.8",
    "helmet": "^6.0.0",
    "ioredis": "^5.3.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.1",
    "nodemailer": "^7.0.12",
    "passport": "^0.7.0",
    "passport-apple": "^2.0.2",
    "passport-google-oauth20": "^2.0.0",
    "stripe": "^20.2.0",
